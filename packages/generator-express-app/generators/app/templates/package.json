{
  "name": "<%= appname %>",
  "version": "1.0.0",
  "description": "<%= appname %>",
  "author": "<%= author %>",
  "license": "<%= license %>",
  "main": "src/server.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "babel src --out-dir app",
    "start:prod": "NODE_ENV=production node -r dotenv/config app/server.js",
    "start:dev": "nodemon -r dotenv/config src/server.js",
    "db:init": "knex migrate:latest; knex seed:run",
    "db:down": "knex migrate:down",
    "test": "npm run test:jest --",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "fast-glob": "^3.2.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.4",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.22",
    "nodemon": "^2.0.4",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/eslint-parser": "*",
    "@babel/preset-env": "^7.13.12",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2"
  }
}
