import React from 'react';
import enzyme from 'enzyme';
import actions from './<%= action.actionName %>';

test('<%= action.actionName %> action test', () => {
    const { request, receive, error } = actions;
    expect(request(1)).toEqual({"payload": {"page": 1}, "type": "request"});
    expect(receive(12345)).toEqual({"payload": {data: 12345}, "type": "receive"});
    expect(error('error')).toEqual(
        {
            "payload": {error: 'error'},
            "meta": {error: 'error', 'type': 'generic'},
            "type": "error"
        }
    );
    expect(error('error', 'major')).toEqual(
        {
            "payload": {error: 'error'},
            "meta": {error: 'error', 'type': 'major'},
            "type": "error"
        }
    );
});
