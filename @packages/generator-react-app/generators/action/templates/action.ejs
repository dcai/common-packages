import { createActions } from 'redux-actions';

const actions = createActions({
  REQUEST: (page = 1) => ({ page }),
  RECEIVE: (data) => ({ data }),
  ERROR: [
    (error) => ({ error }),
    (error, type = 'generic') => ({ error, type }),
  ],
});

